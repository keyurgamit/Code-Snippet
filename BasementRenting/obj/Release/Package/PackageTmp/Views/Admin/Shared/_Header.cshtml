<div class="top-header">
    <div class="container">
        <div class="login-register">
            <ul class="ulemailorcall">
                <li class="licall"><i class='zmdi zmdi-phone'></i> <a href="tel:1-888-444-1234">1-888-444-1234 </a></li>
                <li class="liemail"><i class='zmdi zmdi-email'></i><a href="mailto:info@basementrenting.com"> info@basementrenting.com </a></li>
            </ul>
            <ul class="ulloginregister">
                @if (Session["userid"] != null)
                {
                    @*<li><i class="iicon basement-renting-user"></i> @Html.ActionLink("My Account", "profile", "customer")</li>*@
                    <li class="username"><i class="iicon basement-renting-user"></i> @Html.ActionLink("Hello " + Session["username"].ToString(), "profile", "customer")</li>
                    <li><i class="iicon basement-renting-lock"></i> @Html.ActionLink("Logout", "login-again", "customer")</li>
                }
                else
                {
                    <li><i class="iicon basement-renting-lock"></i> @Html.ActionLink("Login / Register", "login", "customer")</li>
                }
            </ul>
        </div>
    </div>
</div>